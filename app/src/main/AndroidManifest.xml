<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myjtest">
    <!-- 인터넷 권한 선언 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="findView"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MYJTEST"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.example.myjteamproject1.MainView.pathViewActivity"
            android:exported="false" />
        <activity
            android:name="com.example.myjteamproject1.MainView.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.myjteamproject1.PathView.PathActivity" />
        <activity android:name="com.example.myjteamproject1.Menu.MenusActivity"/>
        <activity android:name="com.example.myjteamproject1.Menu.SettingActivity"/>
        <activity android:name="com.example.myjteamproject1.Menu.TransferActivity"/>
        <activity android:name="com.example.myjteamproject1.Menu.BookMarkActivity"/>
    </application>

</manifest>